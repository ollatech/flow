<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	<services>
		<service id="Olla\Flow\DB\NullDB" class="Olla\Flow\DB\NullDB" public="true" >
			<tag name="olla.provider" provider="storage"  /> 
		</service>
		<service id="Olla\Flow\DB\DB" class="Olla\Flow\DB\DB" >
			<argument type="service" id="Olla\Flow\Config" />
			<argument type="service" id="Olla\Flow\Service\Service" />
		</service>
	</services>
</container>